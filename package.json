{
  "name": "nodejs-helper",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/lullaaaby13/nodejs-helper.git",
  "author": "lullaaaby13 <sds901234@naver.com>",
  "license": "MIT",
  "scripts": {},
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.0",
    "@types/supertest": "^6.0.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/$1"
    }
  },
  "dependencies": {
    "puppeteer": "^21.7.0",
    "reflect-metadata": "^0.2.1",
    "rimraf": "^5.0.5",
    "uuid": "^9.0.1"
  }
}
